{
  "name": "@indestructibleeco/api-client",
  "version": "0.1.0",
  "description": "Auto-generated typed API + WebSocket client SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "generate": "openapi-typescript-codegen --input ../../backend/api/openapi.yaml --output src/generated --client fetch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "socket.io-client": "^4.7.0"
  },
  "devDependencies": {
    "openapi-typescript-codegen": "^0.27.0",
    "typescript": "^5.5.0"
  }
}